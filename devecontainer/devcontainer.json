{
  "name": "projeto_bath (Dev)",
  "dockerComposeFile": "docker-compose.yml",
  "service": "postgres",
  "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
  "image": "mcr.microsoft.com/devcontainers/typescript-node:1-20-bullseye",
  "forwardPorts": [3000, 3001, 5555],
  "postCreateCommand": "corepack enable && corepack prepare pnpm@9.15.0 --activate && pnpm install",
  "remoteEnv": {
    "DATABASE_URL": "postgresql://bath:bath@localhost:5432/bath?schema=public",
    "REDIS_URL": "redis://localhost:6379",
    "NEXT_PUBLIC_API_BASE_URL": "http://localhost:3000",
    "NEXT_PUBLIC_API_URL": "http://localhost:3000",
    "NEXT_PUBLIC_DEMO_TOKEN": ""
  }
}