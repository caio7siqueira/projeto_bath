{
  "name": "projeto_bath",
  "private": true,
  "packageManager": "pnpm@9.12.0",
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "test": "turbo run test",
    "test:e2e": "turbo run test:e2e",
    "format": "prettier -w .",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:deploy": "prisma migrate deploy && prisma db seed",
    "openapi:bundle": "pnpm --filter @efizion/api openapi:bundle",
    "openapi:sdk": "pnpm --filter @efizion/contracts sdk:generate",
    "contracts:generate": "pnpm openapi:bundle && pnpm openapi:sdk"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "eslint": "^8.56.0",
    "openapi-typescript": "^7.10.1",
    "openapi-typescript-codegen": "^0.30.0",
    "prettier": "^3.1.1",
    "prisma": "^5.14.0",
    "tsx": "^4.0.0",
    "turbo": "^1.12.5",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "@types/bcryptjs": "^3.0.0",
    "bcryptjs": "^3.0.3",
    "jsonwebtoken": "^9.0.3"
  }
}
